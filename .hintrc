{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off",
        "frame-title": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]",
          "iframe[referrerpolicy=no-referrer-when-downgrade]"
        ]
      }
    ],
    "no-inline-styles": "off",
    "disown-opener": "off"
  }
}